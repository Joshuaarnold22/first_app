<h1>All Questions</h1>
<% @questions.each do |question| %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
          <% raise "Found question without id: #{question.inspect}" unless question.id %>
          <%= link_to question.title, question %>
          <p><%= question.check %></p>
      </h4>
    </div>
  </div>
<% end %>
